{
  "name": "automation",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "test": "npx cypress run && node cucumber-html-report.js",
    "headTest": "npm run test --headed",
    "chromeTest": "npm run test --browser chrome",
    "recordDashBoardTest": "npm run test --record --key fa423514-5a84-42a7-b6a0-b5e7191bba12 --reporter mochawesome",
    "GreenKartTest": "npm run test --spec \"cypress/integration/GreenKart/*\"",
    "Demo": "npm run test --spec \"cypress/integration/GreenKart/*\" --headed --browser chrome",
    "test:Smoke": "npm run test --browser chrome --headed ./node_modules/.bin/cypress-tags run -e TAGS=\"@Smoke1\"",
    "amit": "npx cypress run --spec cypress/integration/examples/BDD/ecomm.feature --browser chrome && node cucumber-html-report.js"

  },
  "cypress-cucumber-preprocessor": {
    "nonGlobalStepDefinitions": true,
    "cucumberJson": {
      "generate": true,
      "outputFolder": "cypress/cucumber-json",
      "filePrefix": "",
      "fileSuffix": ".cucumber"
    }
  },
  "author": "PallaviBhosale",
  "license": "ISC",
  "devDependencies": {
    "cypress": "^10.2.0",
    "cypress-cucumber-preprocessor": "^2.5.5",
    "mocha": "^5.2.0",
    "mochawesome": "^4.0.1",
    "multiple-cucumber-html-reporter": "^1.18.0"
  },
  "dependencies": {
    "cypress-real-events": "^1.7.2",
    "cypress-tags": "^1.0.2",
    "wdio-cucumberjs-json-reporter": "^4.4.3"
  }
}
